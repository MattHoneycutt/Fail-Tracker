<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<connectionStrings>
		<add xdt:Locator="Match(name)" xdt:Transform="Replace" name="FailTrackerData" connectionString="Data Source=|DataDirectory|Test.sdf"/>
	</connectionStrings>

	<system.net>
		<mailSettings>
			<smtp>
				<network host="localhost" port="49999" xdt:Transform="SetAttributes" xdt:Locator="Match(host)" />
			</smtp>
		</mailSettings>
	</system.net>

</configuration>